<%= form_with url: "/companies/#{@company.id}/seeds", method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :available %>
  <%= form.text_field :available %>
  <%= form.label :quantity %>
  <%= form.text_field :quantity %>
  <%= form.submit "Create Seed" %>
<% end  %>
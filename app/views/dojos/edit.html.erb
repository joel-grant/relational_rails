<%= form_with url: "/dojos/#{@dojo.id}", method: :patch, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :open %>
  <%= form.text_field :open %>
  <%= form.label :rating %>
  <%= form.text_field :rating %>
  <%= form.submit "Update Dojo" %>
<% end %>

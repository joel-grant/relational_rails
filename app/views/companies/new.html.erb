<%= form_with url: '/companies', method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :accepting_orders %>
  <%= form.text_field :accepting_orders %>
  <%= form.label :years_active %>
  <%= form.text_field :years_active %>
  <%= form.submit "Create Company" %>
<% end  %>

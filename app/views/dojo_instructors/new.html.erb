<%= form_with url: "/dojos/#{@dojo.id}/instructors", method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :payroll %>
  <%= form.text_field :payroll %>
  <%= form.label :experience %>
  <%= form.text_field :experience %>
  <%= form.submit "Create Instructor" %>
<% end  %>
<%= link_to "Companies Index", "/companies" %>
<p><%= form_with url: '/seeds', method: :get, local: true do |form| %>
  <%= form.label :keyword %>
  <%= form.text_field :keyword %>
  <%= form.submit "Search" %>
<% end %></p>
<% @seeds.return_true.each do |seed| %>
  <h1><%= seed.name %></h1>
  <h3>Available? <%= seed.available %></h3>
  <h3>ID <%= seed.id %></h3>
  <h3>Quantity: <%= seed.quantity %></h3>
<% end %>
